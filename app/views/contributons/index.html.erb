<p id="notice"><%= notice %></p>

<h1>Contributons</h1>

<table class="table">
  <div class="panel-heading">投稿一覧</div>
  <thead>
      <tr>
        <th>Title</th>
        <th>Content</th>
        <th colspan="3"></th>
      </tr>
  </thead>

  <tbody>
    <% @contributons.each do |contributon| %>
    <tr>
      <td><%= contributon.title %></td>
      <td><%= contributon.content %></td>
      <td><button type="button" class="btn btn-primary"><%= link_to 'Show', contributon %></button></td>
      <td><button type="button" class="btn btn-success"><%= link_to 'Edit', edit_contributon_path(contributon) %></button></td>
      <td><button type="button" class="btn btn-danger"><%= link_to 'Destroy', contributon, method: :delete, data: { confirm: 'Are you sure?' } %></button></td>
    </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Contributon', new_contributon_path %>
